{
    "name": "CookieJar",
    "description": "Download browser cookies in the Netscape format.",
    "version": "1.0",
    "manifest_version": 2,
    "icons": {
        "16": "img/cookie16.png",
        "48": "img/cookie48.png",
       "128": "img/cookie128.png"
    },
    "permissions": [
        "cookies",
        "downloads",
        "http://*/*",
        "https://*/*"
    ],
    "browser_action": {
        "default_icon": "img/cookie16.png",
        "default_popup": "popup.html",
        "default_title": "CookieJar"
    }
}